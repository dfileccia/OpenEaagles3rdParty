        <x>   220.0 </x>
        <y>     0.0 </y>
        <z>    80.0 </z>
      </location>
      <orient unit="DEG">
        <roll>   0.0 </roll>
        <pitch>  0.0 </pitch>
        <yaw>    0.0 </yaw>
      </orient>
      <feed>0</feed>
      <feed>1</feed>
      <thruster file="ah1s_rotor">
        <location unit="IN">
          <x>   179.0 </x>
          <y>     0.0 </y>
          <z>   153.0 </z>
        </location>
        <orient unit="DEG">
          <roll>   0.0 </roll>
          <pitch> 90.0 </pitch>
          <yaw>    0.0 </yaw>
        </orient>
        <sense> 1 </sense>
      </thruster>
    </engine>
    <!-- used for linkage -->
    <engine file='electric_1hp_dummy'>
      <location unit="IN">
        <x>   220.0 </x>
        <y>     0.0 </y>
        <z>    80.0 </z>
      </location>
      <orient unit="DEG">
        <roll>   0.0 </roll>
        <pitch>  0.0 </pitch>
        <yaw>    0.0 </yaw>
      </orient>
      <feed>0</feed>
      <feed>1</feed>
      <thruster file="ah1s_tail_rotor">
        <location unit="IN">
          <x> 500.5 </x>
          <y> 0.0 </y>
          <z> 119.0 </z>
        </location>
        <orient unit="DEG">
          <roll>  0.0 </roll>
          <pitch> 0.0 </pitch>
          <yaw>  90.0 </yaw>
        </orient>
        <sense> 1 </sense>
      </thruster>
    </engine>
    <tank type="FUEL" number="0">
      <location unit="IN">
        <x>   155.0 </x>
        <y>     0.0 </y>
        <z>    33.0 </z>
      </location>
      <capacity unit="LBS">    800.0  </capacity>
      <contents unit="LBS">    400.0  </contents>
      <type>JP-4</type>
    </tank>
    <tank type="FUEL" number="1">
      <location unit="IN">
        <x>   195.0 </x>
        <y>     0.0 </y>
        <z>    33.0 </z>
